<odoo>
    <data noupdate="1">
        <record id="project_task_sequence" model="ir.sequence">
            <field name="name">Task Code</field>
            <field name="code">project.task</field>
            <field name="prefix">T/%(month)s/%(range_y)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>
    </data>
</odoo>

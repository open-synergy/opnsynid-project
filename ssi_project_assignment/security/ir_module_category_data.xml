<odoo>
<record id="project_role_configurator_group" model="res.groups">
    <field name="name">Project Role</field>
    <field name="category_id" ref="ssi_project.project_configurator_module_category" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>

<!-- Workflow -->
<record id="project_assignment_workflow_module_category" model="ir.module.category">
    <field name="name">Project Assignment</field>
    <field name="parent_id" ref="ssi_project.project_workflow_module_category" />
    <field name="sequence">4</field>
</record>

<!-- Data Ownership -->
<record
        id="project_assignment_data_ownership_module_category"
        model="ir.module.category"
    >
    <field name="name">Project Assignment</field>
    <field name="parent_id" ref="ssi_project.project_data_ownership_module_category" />
    <field name="sequence">4</field>
</record>
</odoo>
